                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Proveedores.id_proveedor AS Id, Proveedores.nombre_proveedor AS Nombre, Ciudades.nombre AS Ciudad, Barrios.nombre AS Barrio, Proveedores.domicilio_calle + ' ' + CAST(Proveedores.domicilio_numero AS varchar) 
